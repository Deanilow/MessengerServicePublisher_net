{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "AppSettings": {
    "Company": "", // nombre empresa para filtro
    "Definition": "", // definicion separadas por ";" para que tanto BD y GMAIL se filtren las indicadas
    "SenderPhone": "", // numeros de donde se enviara los wsp y se repartira la carga entre ellas
    "GetDataFrom": "", // "Bd"para de base de datos  - "gmail" para que jale de gmail,
    "DeleteDataSendBd": "", // poner en true sole cuando se elimina data enviada por bd

    "SenderGmail": "",
    "ClientIdGmail": "", //clienteId que te otorga gmail
    "ClientSecretGmail": "", // clienteSecret que te otorga gmail
    "NameProyectoGmail": "", // nombre proyecto declarado en gmail console 
    "PermissionGmail": "", // solo poner "True" cuando es un gmail nuevo , para registrar permisos // dejar vacio si no se usa 

    "HostNameRabbitMQ": "", // host de rabbitmq
    "UserNameRabbitMQ": "", //usuario de rabbitmq
    "PasswordNameRabbitMQ": "", // contrasena de rabbitmq

    "NameService": "", // nombre del servicio 
    "cycle": "" // "1" solo se ejecuta 1 vez  - "x" ejecuta cada 6 segundos
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:/Proyectos/Logs/MessengerServicePublisher/Log-32323235-dsdsdsd.com2-.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 120,
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "Worker Service Publisher - .Net 7.0"
    }
  }
}