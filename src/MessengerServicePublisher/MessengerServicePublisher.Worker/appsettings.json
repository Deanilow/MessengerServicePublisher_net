{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "AppSettings": {
    "ClientIdGmail": "",
    "ClientSecretGmail": "",
    "NameProyectoGmail": "",
    "SenderPhone": "",
    "SenderGmail": "",
    "PathTemp": "",
    "Company": "",
