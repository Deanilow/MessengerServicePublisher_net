    "HostNameRabbitMQ": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "D:/Proyectos/Logs/MessengerServiceProducer/log--ads@gmail.com2-.txt",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}",
          "retainedFileCountLimit": 120,
          "rollingInterval": "Day"
        }
      }
    ],
    "Properties": {
      "Application": "ODC Generador de Documentos - .Net 6.0"
    }
  }
}